{
  "name": "Streamline Tools",
  "version": "1.0.0",
  "description": "Streamline Tools for Oracle CPQ Cloud!",
  "permissions": [
    "downloads",
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "scripting",
    "commands"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "jsonpath-1.0.2.js",
        "injected.js",
        "adminCommerceInjected.js",
        "adminConfigInjected.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/streamline16.png",
      "32": "images/streamline32.png",
      "48": "images/streamline48.png",
      "128": "images/streamline128.png"
    }
  },
  "icons": {
    "16": "images/streamline16.png",
    "32": "images/streamline32.png",
    "48": "images/streamline48.png",
    "128": "images/streamline128.png"
  },
  "manifest_version": 3
}
